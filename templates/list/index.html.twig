{% extends 'base.html.twig' %}

{% block title %}Hello ListController!{% endblock %}

{% block body %}

<h1>Liste des races et classes</h1>

<h2>Choisis ta race </h2>

        {% for race in races %}
            <button> <a href="">{{ race.nom }}</a></button>
        {% endfor %}
    

<h2>Choisis ta classe </h2>

    {% for classe in classes %}
            <button> <a href="">{{ classe.nom }}</a></button>
        {% endfor %}

{% endblock %}
